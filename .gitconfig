[push]
  default = simple
[pull]
  default = current
[user]
  name = Rany Keddo
	email = purzelrakete@gmail.com
[core]
  excludesfile = /Users/purzelrakete/.gitignore
  quotepath = false
	compression = 0
[color]
  diff = auto
  status = auto
  branch = auto
[diff]
  rename = copy
  color = auto
[apply]
  whitespace = strip
[pager]
  color = true
[alias]
  co = "checkout"
  ci = "commit"
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[branch]
  autosetuprebase = always
[url "git@github.com:"]
	insteadOf = https://github.com/
